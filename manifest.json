{
  "manifest_version": 2,
  "name": "重定向请求插件",
  "version": "1.0.0",
  "icons": {
    "16": "img/favicon.ico",
    "48": "img/favicon.ico",
    "128": "img/favicon.ico"
  },
  "browser_action": {
    "default_icon": "img/favicon.ico",
    "default_title": "重定向请求插件"
  },
  "permissions": [
    "webRequest",
    "webRequestBlocking",
    "tabs",
    "http://*/*",
    "https://*/*"
  ],
  "content_scripts": [
    {
      "matches": ["http://*/*","https://*/*"],
      "js": ["js/test.js"],
      "run_at": "document_start"
    }
  ],
  "web_accessible_resources": ["js/inject.js"]
}
